script/source = "extends Node2D


func _ready():
	var p: KinematicBody2D = $\"Objects/Player\"

	# the teleport method should be available
	# if successfully loaded from the nativescript
	# library; otherwise, exit
	if not p.has_method(\"teleport\"):
		print(\"existing app: nativescript library did not load correctly\")
		get_tree().quit(1)
"

