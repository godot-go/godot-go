{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "test",
      "type": "cppdbg",
      "request": "launch",
      "program": "${env:HOME}/bin/godot",
      "args": [
        "--verbose",
        "--debug",
        "--path",
        "test/project"
      ],
      "stopAtEntry": false,
      // "customLaunchSetupCommands": [
      //   { "text": "target-run", "description": "run target", "ignoreFailures": false }
      // ],
      "cwd": "${workspaceFolder}",
      "launchCompleteCommand": "exec-run",
      "environment": [
        {
          "name": "GOGC",
          "value": "1"
        },
        {
          "name": "LOG_LEVEL",
          "value": "DEBUG"
        },
        {
          "name": "TEST_USE_GINKGO_WRITER",
          "value": "1"
        },
        {
          "name": "GODEBUG",
          "value": "asyncpremptoff=1,cgocheck=1"
        }
      ],
      "linux": {
        "MIMode": "gdb",
        "miDebuggerPath": "/usr/bin/gdb"
      },
      "osx": {
        "MIMode": "gdb"
      },
      "windows": {
        "MIMode": "gdb",
        "miDebuggerPath": "C:\\MinGw\\bin\\gdb.exe"
      },
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ],
      "sourceFileMap": {
        "./github.com/godot-go/godot-go/": "${workspaceFolder}/",
      }
    },
    {
      "name": "editor",
      "type": "cppdbg",
      "request": "launch",
      "program": "${env:HOME}/bin/godot",
      "args": [
        "--editor",
        "--verbose",
        "--debug",
        "--path",
        "test/project"
      ],
      "stopAtEntry": false,
      // "customLaunchSetupCommands": [
      //   { "text": "target-run", "description": "run target", "ignoreFailures": false }
      // ],
      "cwd": "${workspaceFolder}",
      "launchCompleteCommand": "exec-run",
      "environment": [
        {
          "name": "GOGC",
          "value": "1"
        },
        {
          "name": "LOG_LEVEL",
          "value": "DEBUG"
        },
        {
          "name": "TEST_USE_GINKGO_WRITER",
          "value": "1"
        },
        {
          "name": "GODEBUG",
          "value": "asyncpremptoff=1,cgocheck=1"
        }
      ],
      "linux": {
        "MIMode": "gdb",
        "miDebuggerPath": "/usr/bin/gdb"
      },
      "osx": {
        "MIMode": "gdb"
      },
      "windows": {
        "MIMode": "gdb",
        "miDebuggerPath": "C:\\MinGw\\bin\\gdb.exe"
      },
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ],
      "sourceFileMap": {
        "./github.com/godot-go/godot-go/": "${workspaceFolder}/",
      }
    }
  ]
}